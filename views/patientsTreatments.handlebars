<h1>Sacred Heart Hospital Patient Treatment List</h1>
 <h2>Treatments for {{name}}</h2>
        <br>
       <a href="/" class="btn btn-primary">Return to Portal</a>
       <br>
        <br>
       <a href="/addTreatment/id={{id}}" class="btn btn-primary">Add Treatment</a>
        <br>
        <br>
        <table>
            <tr>
                <th>Treatment Name</th>
                <th>Insured Price</th>
                <th>Uninsured Price</th>
            </tr>
            {{#each results}}
            <tr>
                <td>{{this.Treatment_Type}}</td>
                <td>${{this.Insured_Price}}</td>
                <td>${{this.Uninsured_Price}}</td>
                <td><a href="#" class="btn btn-danger">Remove Treatment</a></td>
                {{!-- <td><button onclick="window.location.href='SHHViewIndividualTreatment.html';">View Details</button></td> --}}
            </tr>
            {{/each}}
        </table>
